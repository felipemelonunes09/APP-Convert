<!DOCTYPE html>
<html>
<head>
	<title> Tabela </title>
</head>
<body>
	<table>
		<thead>
			<% header.forEach(col => { %>
				<th> <%- col %> </th>
			<%})%>
		</thead>
		<tbody>
			<% rows.forEach(row => { %>
				<tr> 
					<% row.forEach(element => {  %> 
						<td>
							<%- element %>
						</td>
					<% }) %>
				</tr>
			<% }) %>
		</tbody>
	</table>
</body>
</html>